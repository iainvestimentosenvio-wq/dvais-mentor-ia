    "@next/bundle-analyzer": "^16.0.1",
    "@playwright/test": "^1.57.0",
    "@types/node": "^20.8.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^10.1.0",
    "eslint": "^8.51.0",
    "eslint-config-next": "^14.2.35",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "next-pwa": "^5.6.0",
    "postcss": "^8.4.31",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.3.5",
    "typescript": "^5.2.2",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "glob": "10.5.0",
    "tmp": "0.2.5"
